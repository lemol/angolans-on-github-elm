{
  "version": 2,
  "env": {
    "AUTH0_DOMAIN": "@auth0_domain",
    "AUTH0_CLIENT_ID": "@auth0_client_id",
    "AUTH0_CLIENT_SECRET": "@auth0_client_secret"
  },
  "rewrites": [
    { "source": "/developers", "destination": "/index.html" },
    { "source": "/repositories", "destination": "/index.html" }
  ]
}
