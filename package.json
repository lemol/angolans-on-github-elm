{
  "name": "angolans-on-github",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently npm:dev:ui-explorer npm:dev:web",
    "dev:web": "parcel -p 8000 static/index.html",
    "dev:ui-explorer": "cd ui-explorer && parcel -p 8800 index.html",
    "build": "parcel build --out-dir public static/index.html"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "elm": "^0.19.1-3",
    "elm-analyse": "^0.16.5",
    "elm-format": "^0.8.2",
    "elm-hot": "^1.1.4",
    "elm-test": "^0.19.1",
    "elm-typescript-interop": "^0.0.15",
    "node-elm-compiler": "^5.0.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.6.0"
  }
}
